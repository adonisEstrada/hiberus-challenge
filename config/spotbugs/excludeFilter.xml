<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- Exclude generated code -->
    <Match>
        <Package name="~.*\.model.*"/>
    </Match>
    <Match>
        <Package name="~.*\.api.*"/>
    </Match>
    <Match>
        <Class name="~.*\..*Application"/>
    </Match>

    <!-- Exclude test classes -->
    <Match>
        <Class name="~.*Test"/>
    </Match>
    <Match>
        <Class name="~.*IT"/>
    </Match>

    <!-- Exclude Lombok generated code -->
    <Match>
        <Bug pattern="EI_EXPOSE_REP,EI_EXPOSE_REP2"/>
        <Class name="~.*"/>
    </Match>
</FindBugsFilter>
